{
	"extends": "@tsconfig/strictest/tsconfig.json",
	"include": ["**/*.ts"],
	"compilerOptions": {
		"types": ["@cloudflare/workers-types", "node"],
		"module": "es2022",
		"moduleResolution": "node",
		"target": "es2021",
		"lib": ["es2021"],
		"noEmit": true,
		"jsx": "react",
		"resolveJsonModule": true
	}
}
